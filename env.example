# Environment Configuration
NODE_ENV=development
PORT=3000

# Database Configuration (if needed later)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=express_api
# DB_USER=postgres
# DB_PASSWORD=password

# JWT Configuration (if needed later)
